# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-key

# Main API Webhook (HF Spaces URL)
MAIN_API_WEBHOOK_URL=https://your-hf-space.hf.space/api/v1/webhook/document-ready
MAIN_API_WEBHOOK_SECRET=your-webhook-secret

# Environment
ENVIRONMENT=production

# CORS - Add your frontend and main API URLs
BACKEND_CORS_ORIGINS=["https://your-frontend.com","https://your-hf-space.hf.space"]

# Chunking Configuration
CHUNK_SIZE=500
CHUNK_OVERLAP=50

# File Upload Limits
MAX_FILE_SIZE=10485760
